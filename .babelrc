{
  "presets": ["stage-0", "react"],
  "plugins": [
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": false
    }]
  ],
  "env": {
    "development": {
      "presets": ["es2015", "react-hmre"]
    },
    "production": {
      "presets": ["es2015"]
    },
    "server": {
      "presets": ["es2015-node4"]
    }
  }
}
