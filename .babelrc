{
  "presets": ["react"],
  "plugins": [
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": false
    }]
  ],
  "env": {
    "client": {
      "presets": [
        ["env", {
          "targets": {
            "browsers": ["> 1%", "last 2 versions", "Firefox ESR"]
          },
          "useBuiltIns": true
        }]
      ],
      "plugins": ["react-hot-loader/babel"]
    },
    "server": {
      "presets": [
        ["env", {
          "targets": {
            "node": "current"
          },
          "useBuiltIns": true
        }]
      ]
    }
  }
}
