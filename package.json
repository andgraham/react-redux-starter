  "scripts": {
    "test": "npm run lint:js && npm run lint:scss",
    "start": "npm run clean && node devServer",
    "start:prod": "NODE_ENV=production node tmp/server.bundle.js",
    "lint:js": "eslint --ext jsx,js .",
    "lint:js:fix": "npm run lint -- --fix",
    "lint:scss": "stylelint -s scss client/**/*.scss",
    "clean": "rm -rf public tmp",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "build:client": "NODE_ENV=production webpack --progress --config webpack/client.prod.js",
    "build:server": "NODE_ENV=production BABEL_ENV=server webpack --progress --config webpack/server.prod.js"
  },
  "dependencies": {
    "ejs": "^2.4.1",
    "express": "^4.13.4",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
