    "browser-sync": "^2.11.1",
    "classnames": "^2.2.3",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.8.0",
    "eslint-config-exeto": "^8.0.1",
    "eslint-plugin-import": "^1.4.0",
    "eslint-plugin-jsx-a11y": "^1.0.2",
    "eslint-plugin-react": "^5.0.1",
